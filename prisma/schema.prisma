datasource db {
  provider = "sqlite"
  url      = "file:./THE-DATABASE--GASP/kiwindb"
}

generator client {
  provider = "prisma-client-js"
}

model Server {
  id      String @id
  prefix  String @default("!")
  members GuildMember[]
}

model GuildMember {
  id       String @id
  server   Server @relation(fields: [serverid], references: [id])
  serverid String
}
